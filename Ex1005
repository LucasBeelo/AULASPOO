
package com.mycompany.ex1005;
import java.util.Scanner; 


public class ex10005 {

 
    public static void main(String[] args) {
        
        Scanner scan = new Scanner (System.in);
        double n1 , n2 , media ; 
        
        System.out.println("Digite a Primeira e a sgunda nota  ");
        n1=scan.nextDouble(); 
        n2=scan.nextDouble(); 
        
        media= ((n1*3.5)+(n2*7.5))/11; 
        
        System.out.println("A media do Alun Ã© : "+media);
        
    }
    
}
