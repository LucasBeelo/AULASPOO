package Ex1007;
import java.util.Scanner
public class Ex1007 {
    

    public static void main(String[] args) {
        Scanner scan = new Scanner (System.in);
       
        int A,B,C,D,DIF ; 
        
        System.out.println("Digite os 4 numeros : ");
        A=scan.nextInt();
        B=scan.nextInt();
        C=scan.nextInt();
        D=scan.nextInt();
        
        DIF=(A*B-C*D);
        
        System.out.println("Valor DIferen√ßa : "+DIF);
    
    }
    
}
